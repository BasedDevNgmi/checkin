{
  "name": "inchecken",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "validate": "npm run lint && npm run typecheck && npm run test && npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ci": "start-server-and-test \"NEXT_PUBLIC_DEV_PREVIEW=true npm run dev\" http://127.0.0.1:3000 \"playwright test tests/e2e\"",
    "test:e2e:pwa": "start-server-and-test \"NEXT_PUBLIC_DEV_PREVIEW=true npm run start\" http://127.0.0.1:3000 \"playwright test tests/e2e/pwa.spec.ts\"",
    "test:a11y": "start-server-and-test \"NEXT_PUBLIC_DEV_PREVIEW=true npm run start\" http://127.0.0.1:3000 \"playwright test tests/e2e/a11y.spec.ts\"",
    "perf:budget": "node ./scripts/check-route-budgets.mjs",
    "perf:lighthouse": "lhci autorun --config=.lighthouserc.json",
    "quality:pwa": "npm run perf:budget && npm run test:e2e:pwa && npm run test:a11y && npm run perf:lighthouse",
    "ops:canary-healthcheck": "node ./scripts/canary-healthcheck.mjs"
  },
  "dependencies": {
    "@ducanh2912/next-pwa": "^10.2.9",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.95.3",
    "@vercel/speed-insights": "^1.3.1",
    "framer-motion": "^12.34.0",
    "lucide-react": "^0.564.0",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "recharts": "^3.7.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.1",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.58.2",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "jsdom": "^28.1.0",
    "start-server-and-test": "^2.1.3",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^4.0.18"
  },
  "overrides": {
    "sourcemap-codec": "npm:@jridgewell/sourcemap-codec@^1.0.0",
    "glob": "^10.4.5",
    "source-map": "^0.7.4"
  }
}
